{
	"name": "MORSE",
	"version": "1.2",
	"manifest_version": 2,
	"description": "morse code", 
	"permissions": ["contextMenus","notifications"],
	"content_scripts": [{
		"matches": ["http://*/*","https://*/*"],
		"js": ["m.js"],
		"all_frames" : true
	}],
	"browser_action": {
		"popup": "popup.html",
		"default_icon": "logo.png"
	}
}